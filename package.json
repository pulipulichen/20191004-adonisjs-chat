  "description": "The fullstack application boilerplate for Adonisjs",
  "main": "index.js",
  "scripts": {
    "0.concurrently": "concurrently --kill-others --names \"adonis,cors,webpack\" \"npm run start\" \"npm run cors-start\" \"npm run w2.webpack-watch-development\"",
    "start": "nodemon server.js",
    "migration": "adonis migration:refresh",
    "test": "node ace test",
    "cors-start": "nodemon ./website-cors/index.js",
    "w1.webpack-build-development": "webpack --mode development",
    "w2.webpack-watch-development": "webpack --watch --mode development --display errors-only",
    "w3.webpack-watch-build-production": "webpack --watch --mode production --display errors-only",
    "w4.webpack-build-production": "webpack --mode production"
  },
  "keywords": [
    "adonisjs",
    "adonis-app"
  ],
